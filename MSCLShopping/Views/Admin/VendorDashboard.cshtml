@model MSCLShopping.Models.Admin

@{
    ViewBag.Title = "VendorDashboard";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


@using (Html.BeginForm("VendorDashboard", "Admin", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
{

    <div class="row">
        <div class="col-md-6 col-lg-3 grid-margin stretch-card">
            <div class="card bg-gradient-primary text-white text-center card-shadow-primary">
                <div class="card-body" style="color:black">
                    <h6 class="font-weight-normal">Total Products</h6>
                    <h2 class="mb-0">@ViewBag.Products </h2>
                </div>
            </div>
        </div>
     
    </div>

   

}



