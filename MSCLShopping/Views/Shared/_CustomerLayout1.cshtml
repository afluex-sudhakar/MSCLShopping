    <script>
        function Search() {
            $("#divload").css({ 'display': '' });
            var value = $('#txtsearch').val();
            window.location.href = "/Customer/ProductList?Status=" + value;
        }
        function SearchMobile() {
            $("#divload").css({ 'display': '' });
            var value = $('#txtsearchmobile').val();
            window.location.href = "/Customer/ProductList?Status=" + value;
        }
        $(document).on('keypress', function (e) {
            if (e.which == 13) {
                Search();
            }
        });

        function login() {
            $(".errortext").removeClass("errortext");
            if ($('#LoginID').val() == '') {
                $("#LoginID").addClass('errortext');
                $('#LoginID').focus();
                return false;
            }
            if ($('#Password').val() == '') {
                $("#Password").addClass('errortext');
                $('#Password').focus();
                return false;
            }

            $.ajax({
                url: "/Home/LoginCustomer",
                Type: "Get",
                DataType: "JSON",
                data: { LoginID: $("#LoginID").val(), Password: $("#Password").val() },
                success: function (data) {
                    if (data.Result == "1") {
                        window.location.reload();
                    }
                    else {
                        alert(data.Result);
                        window.location.href = "/Home/Index";
                    }
                }
            });
        }
        function logout() {
            $.ajax({
                url: "/Home/Logout",
                Type: "Get",
                DataType: "JSON",
                data: {},
                success: function (data) {
                    if (data.Result == "1") {
                        window.location.href = "/Home/Index";
                    }
                }
            });
        }
        

